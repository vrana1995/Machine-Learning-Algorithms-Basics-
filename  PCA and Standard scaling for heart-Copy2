   "source": [
    "\"\"\"\" Here we remove the outliers for the Cholesterol\n",
    "     and use those values that lie between Mean ± 3* Std\n",
    "\"\"\"\n",
    "df1= df1[df1.Cholesterol>(df1.Cholesterol.mean()-3*df1.Cholesterol.std())]\n",
    "df1= df1[df1.Cholesterol<(df1.Cholesterol.mean()+3*df1.Cholesterol.std())]"
   ]
  },
